<!DOCTYPE html>
<html>
<head>
  <title>Providers List</title>
</head>
<body>
  <h1>Providers By Specialty List</h1>
  <table border="1">
    <thead>
      <tr>
        <th>Provider ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Provider's Specialty</th>
      </tr>
    </thead>
    <tbody>
      <% ProviderSpecialty.forEach(row => { %>
        <tr>
          <td><%= row.provider_id %></td>
          <td><%= row.first_name %></td>
          <td><%= row.last_name %></td>
          <td><%= row.provider_specialty %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
